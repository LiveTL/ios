abstract_target 'livetl' do
  use_frameworks!
  inhibit_all_warnings!

  target 'ios' do
    platform :ios, '14.0'

    #pod 'common', :git => 'https://github.com/livetl/common'

    pod 'Eureka'
    pod 'FontBlaster'
    pod 'Kingfisher'
    pod 'Neon'
    pod 'SCLAlertView'
    pod 'XCDYouTubeKit'

    pod 'RxCocoa'
    pod 'RxDataSources'
    pod 'RxFlow'
    pod 'RxSwift'
    pod 'SwiftDate'
    pod 'SwiftyUserDefaults'

    pod 'FLEX', :configuration => 'DEBUG'
  end
end
